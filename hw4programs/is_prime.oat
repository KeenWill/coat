global list = int[] {3, 5, 7, 8, 11, 16, 17, 21};

bool isPrime (int n) {
  if (n < 2) {
    return false;
  }

  for (var i = 2; i < n; i = i + 1;)
  {
    var cur_num = n;
    while (cur_num >= i) {
      cur_num = cur_num - i;
    }

    if (cur_num == 0) {
      return false;
    }
  }
  return true;
}

int program (int argc, string[] argv) {
  var answer = 0;
  for (var i = 0; i < 8; i = i + 1;)
  {
    if(isPrime(list[i])) {
      answer = answer + 1;
    }
  }
  return answer;
}
